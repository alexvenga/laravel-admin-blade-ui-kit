{
    "name": "alexvenga/laravel-admin-blade-ui-kit",
    "description": "Laravel blade admin-panel components",
    "keywords": [
        "AlexVenga",
        "laravel",
        "laravel-admin-blade-ui-kit"
    ],
    "homepage": "https://github.com/alexvenga/laravel-admin-blade-ui-kit",
    "license": "MIT",
    "authors": [
        {
            "name": "AlexVEnga",
            "email": "bubnov.alexey@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AlexVenga\\LaravelAdminBladeUIKit\\": "src",
            "AlexVenga\\LaravelAdminBladeUIKit\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexVenga\\LaravelAdminBladeUIKit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexVenga\\LaravelAdminBladeUIKit\\LaravelAdminBladeUIKitServiceProvider"
            ],
            "aliases": {
                "LaravelAdminBladeUIKit": "AlexVenga\\LaravelAdminBladeUIKit\\Facades\\LaravelAdminBladeUIKit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}