{
    "name": "alexvenga/laravel-admin-blade-ui-kit",
    "description": "Laravel blade admin-panel components",
    "keywords": [
        "laravel",
        "blade",
        "ui",
        "kit",
        "ui kit"
    ],
    "homepage": "https://github.com/alexvenga/laravel-admin-blade-ui-kit",
    "license": "MIT",
    "authors": [
        {
            "name": "AlexVEnga",
            "email": "bubnov.alexey@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.11.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "AlexVenga\\LaravelAdminBladeUIKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexVenga\\LaravelAdminBladeUIKit\\LaravelAdminBladeUIKitServiceProvider"
            ],
            "aliases": {
                "LaravelAdminBladeUIKit": "AlexVenga\\LaravelAdminBladeUIKit\\Facades\\LaravelAdminBladeUIKit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
